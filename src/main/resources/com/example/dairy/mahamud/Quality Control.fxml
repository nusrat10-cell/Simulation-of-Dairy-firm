<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dairy.mahamud.QualityControl">
   <children>
      <TextField fx:id="qcinputtextfieldfx" layoutX="64.0" layoutY="43.0" text="input test result" />
      <Label fx:id="qclabelmassagefx" layoutX="90.0" layoutY="94.0" text="test parameters" />
      <RadioButton fx:id="qcpassradbuttonfx" layoutX="64.0" layoutY="162.0" mnemonicParsing="false" text="pass" />
      <RadioButton fx:id="qcfailradbuttonfx" layoutX="184.0" layoutY="162.0" mnemonicParsing="false" text="fail" />
      <Label fx:id="errorlab" layoutX="232.0" layoutY="200.0" text="error" />
      <Button layoutX="132.0" layoutY="272.0" mnemonicParsing="false" onAction="#qcqualityconfirmbuttonfx" text="quality confirm" />
      <Button layoutX="16.0" layoutY="14.0" mnemonicParsing="false" onAction="#qcbackbuttonfx" text="Back" />
   </children>
</AnchorPane>
